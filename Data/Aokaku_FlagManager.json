      }
    ]
  },
  "Modifiers": {
    "AokakuAidFlagWindowModifier": {
      "Duration": 1.5,
      "ModifierStacking": "Refresh",
      "OnAdded": [
        {
          "$type": "HandleAnimatorZoneTagsAction",
          "AnimatorStateTags": [
            "AokakuAidFlag"
          ],
          "IsAdd": true,
          "Target": "Self"
        }
      ],
      "OnRemoved": [
        {
          "$type": "HandleAnimatorZoneTagsAction",
          "AnimatorStateTags": [
            "AokakuAidFlag"
          ],
          "IsAdd": false,
          "Predicates": [
